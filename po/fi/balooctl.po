msgid "balooctl"
msgstr "balooctl"

#: main.cpp:68
msgid "Vishesh Handa"
msgstr "Vishesh Handa"

#: main.cpp:68
msgid "Maintainer"
msgstr "Ylläpitäjä"

#: main.cpp:74
msgid "The command to execute"
msgstr "Suoritettava komento"

#: main.cpp:76
msgid "Print the status of the indexer"
msgstr "Tulosta indeksoijan tila"

#: main.cpp:77
msgid "Enable the file indexer"
msgstr "Ota tiedostoindeksoija käyttöön"

#: main.cpp:78
msgid "Disable the file indexer"
msgstr "Poista tiedostoindeksoija käytöstä"

#: main.cpp:79
msgid "Start the file indexer"
msgstr "Käynnistä tiedostoindeksoija"

#: main.cpp:80
msgid "Stop the file indexer"
msgstr "Pysäytä tiedostoindeksoija"

#: main.cpp:81
msgid "Restart the file indexer"
msgstr "Käynnistä tiedostoindeksoija uudelleen"

#: main.cpp:82
msgid "Suspend the file indexer"
msgstr "Keskeytä tiedostojen indeksointi"

#: main.cpp:83
msgid "Resume the file indexer"
msgstr "Jatka tiedostojen indeksointia"

#: main.cpp:84
msgid "Check for any unindexed files and index them"
msgstr "Tarkista indeksoimattomat tiedostot, ja indeksoi ne"

#: main.cpp:85
msgid "Index the specified files"
msgstr "Indeksoi määritetyt tiedostot"

#: main.cpp:86
msgid "Forget the specified files"
msgstr "Unohda määritetyt tiedostot"

#: main.cpp:87
msgid "Modify the Baloo configuration"
msgstr "Muuta Baloon asetuksia"

